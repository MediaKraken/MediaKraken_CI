# Execution
exec\s*\(.*\$.*\)
passthru\s*\(.*\)
popen\s*\(.*\$.*\)
shell_exec\s*\(.*\$.*\)
system\s*\(.*\$.*\)
`.*\$.*`
#Otherstuffs
#XSS signature needs to stop matching before LF when color=on #bug(1)
echo.*\$_.*\[.*\]
eval\s*\(.*\$.*\)
#SQLi signature needs to stop matching before LF when color=on #bug(1)
mysql_query\s*\(.*\$.*\)
WHERE\s+.*=.*\$[^; ]+
(include|include_once|require|require_once)\s*\(.*\$.*\)
print.*param\s*\(.*\);
